{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "linebreak-style": "off",
    "consistent-return": "off",
    "quotes": ["error", "single"],
    "comma-dangle": "off"
  }
}
